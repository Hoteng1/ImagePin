@{
    ViewBag.Title = "Rating";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Rating</h2>
<p>
    <a class="btn btn-primary" data-toggle="collapse" href="#collapseImage" aria-expanded="false" aria-controls="collapseExample">
        Рейтинг Изображений
    </a>
    <a class="btn btn-primary" data-toggle="collapse" href="#collapseUser" aria-expanded="false" aria-controls="collapseExample">
        Рейтинг Пользователя
    </a>
   
    <div class="collapse" id="collapseUser">
        <div class="card card-block">
            @Html.Action("RatingUser")
        </div>
    </div>

    <div class="collapse" id="collapseImage">
        <div class="card card-block">
            @Html.Action("RatingImage")
        </div>
    </div>
   

